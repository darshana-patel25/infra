{% comment %}
  {% include 'breadcrumb' %}
{% endcomment %}

<div class="grid grid--full about_us">
  
  <div class="wrapper">

    <div class="grid-item about_us_content text-center large--seven-tenths push--large--one-sixth">

      <h1>{{ page.title }}</h1>

      <div class="rte">
        {{ page.content }}
      </div>

    </div>

    {% section 'image-with-text' %}
  </div>
  
  <div class="image_bar_section">
    <div class="wrapper">
      {% section 'image-bar' %}
    </div>
  </div>
  
  <div class="wrapper">
    {% section 'about-image-with-text' %}
  </div>

</div>